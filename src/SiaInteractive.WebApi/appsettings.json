{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cors": {
    "OriginCors": [
      "http://localhost:4200",
      "http://localhost:5600"
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "File": {
    "AllowedExtensions": [ ".jpg", ".jpeg", ".png", ".webp", ".bmp" ],
    "AllowedContentTypes": [ "image/jpeg", "image/png", "image/webp", "image/bmp" ],
    "MaxFileSizeInBytes": 1048576 // 1 MB
  }
}
